{
  "url": "https://feec3dhub.herokuapp.com/",
  "server": {
    "host": "0.0.0.0"
  },
  "database": {
    "client": "mysql",
    "debug": false,
    "connection": {
      "host": "127.0.0.1",
      "user": "your_database_user",
      "password": "your_database_password",
      "database": "your_database_name"
    }
  },
  "mail": {
    "transport": "SMTP",
    "options": {
      "service": "Mailgun",
      "auth": {
        "user": "user",
        "pass": "pass"
      }
    }
  },
  "logging": {
    "transports": [
      "file",
      "stdout"
    ]
  },
  "process": "local",
  "paths": {
    "contentPath": "content/"
  }
}
